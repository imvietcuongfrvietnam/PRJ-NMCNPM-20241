            </ImageView>
         </children>
      </StackPane>
      <StackPane layoutX="422.5" layoutY="350.0">
         <children>
            <TextField fx:id="firstNameField" prefHeight="75.0" prefWidth="312.5" promptText="Tên" style="-fx-background-radius: 15; -fx-background-color: rgba(255, 255, 255); -fx-text-fill: #002060;">
