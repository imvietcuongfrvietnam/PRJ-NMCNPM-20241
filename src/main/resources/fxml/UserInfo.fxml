<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Blend?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="275.0" prefWidth="336.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23" fx:controller="myapp.controller.UserInfoController">  <!-- Chỉ để spacing ở đây, không có padding -->

    <ImageView fx:id="imgView" fitHeight="100" fitWidth="100" preserveRatio="true" />
    <Label fx:id="lblSoCMND" alignment="CENTER" centerShape="false" prefHeight="38.0" prefWidth="244.0" text="Số CMND: ">
      <opaqueInsets>
         <Insets bottom="1.0" left="1.0" right="1.0" top="1.0" />
      </opaqueInsets>
      <cursor>
         <Cursor fx:constant="DEFAULT" />
      </cursor>
      <effect>
         <Blend />
      </effect>
      <VBox.margin>
         <Insets top="1.0" />
      </VBox.margin>
      <padding>
         <Insets left="1.0" />
      </padding></Label>
    <Label fx:id="lblTen" prefHeight="23.0" prefWidth="79.0" text="Họ Tên: " textAlignment="CENTER" />
    <Label fx:id="lblEmail" text="Email: " />
    <Label fx:id="lblQueQuan" text="Quê Quán: " />
    <Label fx:id="lblDienThoai" text="Điện Thoại: " />
    <Button fx:id="btnReturn" alignment="CENTER" contentDisplay="RIGHT" defaultButton="true" nodeOrientation="RIGHT_TO_LEFT" prefHeight="22.0" prefWidth="300.0" text="Trở Về Màn Hình Chính" textAlignment="RIGHT" textFill="#0b091f" />
   <Accordion /> <!-- Thêm fx:id ở đây -->
</VBox>
